{
  "name": "@lazarv/react-server",
  "version": "0.0.0",
  "description": "The easiest way to build React apps with server-side rendering",
  "type": "module",
  "module": "server/index.mjs",
  "types": "server/index.d.ts",
  "bin": "bin/cli.mjs",
  "exports": {
    "./client": {
      "default": "./client/index.jsx",
      "types": "./client/index.d.ts"
    },
    "./config": {
      "default": "./config/index.mjs",
      "types": "./config/index.d.ts"
    },
    "./error-boundary": {
      "default": "./server/error-boundary.jsx",
      "types": "./server/error-boundary.d.ts"
    },
    "./memory-cache": {
      "default": "./memory-cache/index.mjs",
      "types": "./memory-cache/index.d.ts"
    },
    "./navigation": {
      "default": "./client/navigation.jsx",
      "types": "./client/navigation.d.ts"
    },
    "./http-context": {
      "default": "./server/http-context.mjs"
    },
    "./router": {
      "default": "./server/router.jsx",
      "types": "./server/router.d.ts"
    },
    "./remote": {
      "default": "./server/remote.jsx",
      "types": "./server/remote.d.ts"
    },
    "./prerender": {
      "default": "./server/prerender.jsx",
      "types": "./server/prerender.d.ts"
    },
    "./dev": {
      "default": "./lib/dev/index.mjs",
      "types": "./lib/dev/index.d.ts"
    },
    "./node": {
      "default": "./lib/start/node.mjs",
      "types": "./lib/start/node.d.ts"
    },
    "./file-router": {
      "default": "./lib/plugins/file-router/entrypoint.jsx"
    },
    "./client/*": "./client/*",
    "./server/*": "./server/*",
    "./lib/*": "./lib/*",
    "./*": "./*",
    ".": {
      "default": "./server/index.mjs",
      "types": "./server/index.d.ts"
    }
  },
  "scripts": {},
  "keywords": [
    "react",
    "ssr",
    "esm",
    "server"
  ],
  "author": "lazarv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lazarv/react-server.git"
  },
  "bugs": {
    "url": "https://github.com/lazarv/react-server/issues"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-react-jsx-self": "^7.25.9",
    "@babel/plugin-transform-react-jsx-source": "^7.25.9",
    "@hattip/adapter-node": "^0.0.34",
    "@hattip/compose": "^0.0.34",
    "@hattip/cookie": "^0.0.34",
    "@hattip/core": "^0.0.34",
    "@hattip/cors": "^0.0.34",
    "@hattip/headers": "^0.0.34",
    "@hattip/multipart": "^0.0.34",
    "@inquirer/prompts": "^7.0.0",
    "@inquirer/search": "^3.0.0",
    "@mdx-js/rollup": "^3.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "acorn": "^8.10.0",
    "acorn-loose": "^8.4.0",
    "algoliasearch": "^5.10.2",
    "ansi-regex": "^6.0.1",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "deep-eql": "^5.0.2",
    "esbuild": "^0.19.3",
    "escodegen": "^2.1.0",
    "estraverse": "^5.3.0",
    "fast-glob": "^3.2.12",
    "filesize": "^10.0.12",
    "micromatch": "^4.0.5",
    "mime": "^3.0.0",
    "module-alias": "^2.2.3",
    "open": "^9.1.0",
    "parse5": "^7.1.2",
    "picocolors": "^1.0.1",
    "pino": "^8.14.1",
    "react": "0.0.0-experimental-9ff42a87-20250130",
    "react-dom": "0.0.0-experimental-9ff42a87-20250130",
    "react-is": "0.0.0-experimental-9ff42a87-20250130",
    "react-property": "^2.0.2",
    "react-refresh": "^0.14.2",
    "react-server-dom-webpack": "0.0.0-experimental-9ff42a87-20250130",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "sass": "^1.63.6",
    "semver": "^7.6.3",
    "strip-ansi": "^7.1.0",
    "style-to-js": "^1.1.12",
    "vite": "6.1.0"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}
